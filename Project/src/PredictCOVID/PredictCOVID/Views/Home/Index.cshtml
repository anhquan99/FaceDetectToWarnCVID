
@{
    ViewBag.Title = "Index";
}
<style>
    .tableImg {
        max-height: 100px;
        max-width: 100px;
    }
</style>
<div class="container">
    <div class="row">
        <table class="table">
            <thead class="thead-dark" style="background:#343a40; color: #fff">
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">NAME</th>
                    <th scope="col">PERCENTAGE</th>
                    <th scope="col">IMAGE</th>
                    <th scope="col">STATUS</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < ViewBag.data.Count; i++)
                {
                <tr>
                    <th scope="row">@(i + 1)</th>
                    <td>@ViewBag.data[i].NAME</td>
                    <td>@ViewBag.data[i].PERDICT_PER%</td>
                    <td><img class="tableImg" src="@Url.Action("showImg", "img", new { id = ViewBag.data[i].NAME })"/></td>
                    <td>@if (ViewBag.data[i].STATUS == true)
                    {
                        <span style="color:red">Effected</span>
                    }
                    else
                    {
                        <span style="color:blue">Predic</span>
                    }
                    </td>
                </tr>
                }
            </tbody>
        </table>
    </div>
</div>

