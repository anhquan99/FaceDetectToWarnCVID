
@{
    ViewBag.Title = "AddEffectedForm";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<form class="form-horizontal" action="@Url.Action("AddEffectedPost", "Home")" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label class="col-sm-2 control-label">Name</label>
        <div class="col-sm-10">
            <input type="hidden"  name="name"  value="@ViewBag.data.NAME">
            <p>@ViewBag.data.NAME</p>
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 control-label">Files</label>
        <div class="col-sm-10">
            <input type="datetime-local" name="timeEffted">
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <button type="submit" class="btn btn-default">ADD</button>
        </div>
    </div>
</form>


